<div class="govuk-radios {ctx.inline ? 'govuk-radios--inline' : ''}}">
    {% ctx.values.forEach(function(item) { %}
    <div class="govuk-radios__item" ref="wrapper">
        <{{ctx.input.type}}
        ref="input"
        {% for (var attr in ctx.input.attr) { %}
        {{attr}}="{{ctx.input.attr[attr]}}"
        {% } %}
        value="{{item.value}}"
        {% if (ctx.value === item.value || (typeof ctx.value === 'object' && ctx.value.hasOwnProperty(item.value) && ctx.value[item.value])) { %}
        checked=true
        {% } %}
        id="{{ctx.id}}{{ctx.row}}-{{item.value}}"
        >
        <label class="govuk-label govuk-radios__label" for="{{ctx.id}}{{ctx.row}}-{{item.value}}">
            <span>{{ctx.t(item.label)}}</span>
        </label>
    </div>
    {% }) %}
</div>
