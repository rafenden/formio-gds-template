{
  "name": "@digitalpatterns/formio-gds-template",
  "version": "1.0.5",
  "description": "Gov UK design system formio templates",
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "scripts": {
    "test:coverage": "nyc --reporter=text mocha --reporter spec './{,!(node_modules)/**/}*.spec.js'",
    "test": "mocha --require ts-node/register --reporter spec './{,!(node_modules)/**/}*.spec.ts'",
    "watch": "tsc -w",
    "build": "tsc && gulp templates",
    "lint": "tslint -p .",
    "prepublish": "npm run build"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DigitalPatterns/formio-gds-template.git"
  },
  "keywords": [
    "formio"
  ],
  "author": "Amin Mohammed-Coleman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DigitalPatterns/formio-gds-template/issues"
  },
  "homepage": "https://github.com/DigitalPatterns/formio-gds-template#readme",
  "dependencies": {
    "formiojs": "^4.6.2",
    "govuk-frontend": "^3.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/ejs": "^2.6.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.2",
    "@types/sinon": "^7.5.0",
    "chai": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-insert": "^0.5.0",
    "gulp-rename": "^1.4.0",
    "gulp-template": "^5.0.0",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "pre-commit": "^1.2.2",
    "sinon": "^7.5.0",
    "ts-node": "^8.4.1",
    "ts-sinon": "^1.0.24",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  }
}
